    [FileSystemEntityId] INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, 
    [Name] NVARCHAR(255) NOT NULL COLLATE NOCASE,
    [ParentFileSystemEntityId] INTEGER NULL,
    [IsFile] BIT NOT NULL,
